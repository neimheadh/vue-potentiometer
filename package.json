{
  "name": "vue-potentiometer",
  "homepage": "https://neimheadh.github.io/vue-potentiometer/",
  "version": "0.2.2",
  "license": "MIT",
  "keywords": [
    "vue",
    "vuejs",
    "input",
    "potentiometer",
    "responsive"
  ],
  "main": "dist/potentiometer.common.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "rm -f js/* & rm -f css/* & vue-cli-service build --no-clean --dest .",
    "lint": "vue-cli-service lint",
    "build-bundle": "vue-cli-service build --target lib --name potentiometer src/Potentiometer.vue"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neimheadh/vue-potentiometer.git"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "lodash.defaultsdeep": "^4.6.1",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.1",
    "@vue/cli-service": "^3.1.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "browser": {
    "./Potentiometer": "./src/Potentiometer.vue"
  }
}
