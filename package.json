{
  "name": "vue-potentiometer",
  "homepage": "https://neimheadh.github.io/vue-potentiometer/",
  "version": "0.3.1",
  "license": "MIT",
  "keywords": [
    "vue",
    "vuejs",
    "input",
    "potentiometer",
    "responsive"
  ],
  "main": "dist/potentiometer.common.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "rm -f js/* & rm -f css/* & vue-cli-service build --no-clean --dest .",
    "lint": "vue-cli-service lint",
    "build-bundle": "vue-cli-service build --target lib --name potentiometer src/Potentiometer.vue"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neimheadh/vue-potentiometer.git"
  },
  "dependencies": {
    "core-js": "^3.6.0",
    "lodash": "^4.17.14",
    "lodash.defaultsdeep": "^4.6.1",
    "serialize-javascript": ">=2.1.1",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.5.9",
    "@vue/cli-plugin-eslint": "4.5.9",
    "@vue/cli-service": "4.5.9",
    "babel-eslint": "10.1.0",
    "eslint": "7.14.0",
    "eslint-plugin-vue": "7.1.0",
    "node-sass": "5.0.0",
    "sass-loader": "10.1.0",
    "vue-template-compiler": "2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "browser": {
    "./Potentiometer": "./src/Potentiometer.vue"
  }
}
